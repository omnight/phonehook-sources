          <set id="008579021267571973" name="address" value="/(.+)/" decode="XML" />
        </set>
        <set id="06583263887471584" name="number" value="/href=&quot;tel:(\d+)&quot;/" />
        <set id="09834917239914611" name="url" value="/&lt;h2[^&gt;]*class=&quot;[^&quot;]*listing-name[^&gt;]*&gt;\s*&lt;a[^&gt;]*href=&quot;(.*?)&quot;/" decode="RelativeURL" />
        <set id="05979393062202354" name="#type" value="/^&lt;div class=&quot;(.*?)&quot;/">
