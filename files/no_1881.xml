        <set id="06066502444273367" name="title" value="Name" />
        <set id="027979630662729615" name="value" value="/(.+)/" />
      </set>
      <set id="012107471982463314" name="field" value="/&lt;h2&gt;Adresse&lt;/h2&gt;\s*&lt;ul&gt;(.*?)&lt;/ul&gt;/" decode="XML">
        <set id="0381606800909311" name="stitle" value="address" />
        <set id="046390287394880314" name="title" value="Address" />
        <set id="06681203954321775" name="#remove-map" value="/&lt;li&gt;&lt;a[^&gt;]*&gt;Kart.*?&lt;/li&gt;/" decode="RegexReplace">
          <set id="024104173347360813" name="value" value="/^\s*|&lt;[^&gt;]*&gt;|\s*$&#xA;\s*\n\s*/" decode="RegexMultiReplace" formatstring="&#xA;, " />
